{
  "name": "ocsnippetsdavidev",
  "displayName": "Opencart Snippetsone Davidev ",
  "description": "OpenCart snippets for Visual Studio Code. For contact [Contact Us](http://www.davidev.it/)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "icon": "icon.jpg",
  "publisher": "davidev",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/engine.json"
      },
      {
        "language": "php",
        "path": "./snippets/library.json"
      },
      {
        "language": "php",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "xml",
        "path": "./snippets/xmlsnippets.json"
      }
    ],
    "commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
  },
  "scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
